FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    top_patch		//outlet for our case
    {
        type            fixedValue;
	value		uniform 0;
    }

    wall_patch
    {
        type            zeroGradient;
    }

    bottom_patch	// inlet for our case
    {
        type            zeroGradient;
    }

}

